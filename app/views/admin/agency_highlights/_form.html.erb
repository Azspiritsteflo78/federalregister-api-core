<%= f.inputs do %>
  <%= f.input :section_header %>
  <%= f.input :title %>
  <%= f.input :abstract, :as => :text %>
  <%= f.input :agency_id, :as => :select, :collection => @agency_highlight.entry.agencies %>
  <%= f.input :published %>
  <%= f.input :highlight_until %>
<% end %>
<%= f.hidden_field :entry_id %>

<%= f.actions do %>
  <%= f.action :submit, :label => 'Save', :button_html => {:class => "btn btn-large btn-success"} %>
  <%= link_to 'Cancel', unprocessed_admin_agency_names_path %>
<% end %>
